#include <iostream>
#include <cstring>
using namespace std;

int main() {

char s1[100],s2[100],s3[200];

cin.getline(s1, 100);
cin.getline(s2, 100);

int i,j;

for(i = 0; s1[i] != '\0' ; i++)
{
 s3[i] = s1[i];
}

for(j = 0; s2[j] != '\0'; j++)
 {
 s3[i+j] = s2[j];
     
 }



s3[i + j] = '\0';
cout << s3 << endl;

    return 0;
}
